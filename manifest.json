{
	"manifest_version": 2,
	"name": "Tab Deduper",
	"version": "1.1.0",
	"description": "Removes / closes duplicate tabs, replacing it with a matching existing tab.",
	"developer": {
		"name": "nosoop",
		"url": "https://github.com/nosoop/"
	},
	"icons": {
		"48": "icon.svg",
		"96": "icon.svg",
		"128": "icon.svg",
		"256": "icon.svg"
	},
	"background": {
		"scripts": [ "background.js" ]
	},
	"permissions": [ "tabs", "cookies", "notifications" ]
}